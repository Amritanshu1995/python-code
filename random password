import string
import random

leng = int(input("enter the length of the string:- "))

'''lower = string.ascii_lowercase
upper = string.ascii_uppercase
digit = string.digits
symbol = string.punctuation'''

# password = lower + upper+digit+symbol
str = string.printable
pwd = random.sample(str, leng)
password = "".join(pwd)
print(password)
