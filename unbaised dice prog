n = int(input("enter the number: "))
count = 0
if(n>0 and n<=12):
    for i in range(1, 7):
        for j in range(1, 7):
            if(i+j == n):
                print((i, j))
                count = count+1
    print(f"The number of formation is {count}")

else:
    print("Please enter the number greater than 0 and less than of equal to 12")
